#: The_Settlers_of_Light/scenarios/9_1_Elvish_Council.cfg:7
msgid "Talking with Pointears"
msgstr ""

#. [side]: type=Elvish Councillor, id=Eglandor
#. [side]: type=Elvish Elite Ranger, id=Neglandur
#. [side]: type=Elvish Great Druid, id=Celylia
#: The_Settlers_of_Light/scenarios/9_1_Elvish_Council.cfg:53
#: The_Settlers_of_Light/scenarios/9_1_Elvish_Council.cfg:73
#: The_Settlers_of_Light/scenarios/9_1_Elvish_Council.cfg:93
#: The_Settlers_of_Light/scenarios/9_2_Forest_Raiders.cfg:56
#: The_Settlers_of_Light/scenarios/9_2_Forest_Raiders.cfg:82
